{
  "name": "mtx-pdf-editor",
  "displayName": "MTX PDF Editor",
  "description": "Feature-rich React-based PDF editor with AI capabilities for VS Code",
  "version": "0.0.1",
  "publisher": "MTX-Project",
  "author": {
    "name": "MrTomXxX",
    "url": "https://github.com/MrT0mX"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MTXPr0ject/mtx-pdf-editor"
  },
  "bugs": {
    "url": "https://github.com/MTXPr0ject/mtx-pdf-editor/issues"
  },
  "homepage": "https://github.com/MTXPr0ject/mtx-pdf-editor#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "pdf",
    "editor",
    "viewer",
    "mtx",
    "ai"
  ],
  "icon": "media/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mtx-pdf-editor.openPdf",
        "title": "Open MTX PDF Editor",
        "icon": "$(file-pdf)"
      },
      {
        "command": "mtx-pdf-editor.exportPdf",
        "title": "Export PDF",
        "icon": "$(export)"
      },
      {
        "command": "mtx-pdf-editor.analyzeContent",
        "title": "Analyze PDF Content",
        "icon": "$(search)"
      },
      {
        "command": "mtx-pdf-editor.extractText",
        "title": "Extract Text with AI",
        "icon": "$(symbol-text)"
      }
    ],
    "customEditors": [
      {
        "viewType": "mtx-pdf-editor.pdfEditor",
        "displayName": "MTX PDF Editor",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": {
      "title": "MTX PDF Editor",
      "properties": {
        "mtx-pdf-editor.firstInstall": {
          "type": "boolean",
          "default": true,
          "description": "Indicates if this is the first installation"
        },
        "mtx-pdf-editor.aiFeatures": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered features"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node ./test/runTest.js",
    "build:webview": "vite build webview-ui",
    "dev:webview": "vite webview-ui",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.9.179",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "zustand": "^4.4.1",
    "tesseract.js": "^4.1.1",
    "openai": "^4.0.0",
    "@dqbd/tiktoken": "^1.0.7",
    "fabric": "^5.3.0",
    "@react-pdf/renderer": "^3.1.12"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.47.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vsce": "^2.15.0"
  }
}